<div data-vue-hawksearch-component="results" data-vue-hawksearch-config="hawksearch-config">
    <div class="hawk">
        <div class="hawk__body">
            <results></results>
        </div>
    </div>
</div>

<script id="vue-hawksearch-results" type="x-template">
    <div class="hawk-results" @click="onClick">
        <banner zone="FeaturedTop" />

        <autocorrect-suggestions />

        <search-results-label />

        <selections />

        <template v-if="searchError">
            <span>{{ $t('response_error_generic') }}</span>
        </template>
        <template v-else-if="results && results.length == 0">
            <span>{{ $t('No Results') }}</span>
        </template>
        <template v-else-if="!waitingForInitialSearch">
            <tabs />

            <div class="hawk-results__top-tool-row">
                <tool-row />
            </div>

            <listing-type :listing-types="['grid', 'list']" @typeChanged="onListingChange" />

            <result-listing :type="listingType" />

            <div class="hawk-results__bottom-tool-row">
                <tool-row />
            </div>
        </template>

        <banner zone="FeaturedBottom" />
        <banner zone="FeaturedBottom2" />

    </div>
</script>

<script id="vue-hawksearch-result-listing" type="x-template">
    <div :class="'hawk-results__listing ' + listingTypeClass">
        <template v-if="loadingResults">
            <spinner></spinner>
        </template>
        <template v-if="results && results.length">
            <result-item v-show="type == 'grid'" v-for="result in results" :key="result.DocId" :result="result" template-override="#vue-hawksearch-result-item-grid"></result-item>
            <result-item v-show="type == 'list'" v-for="result in results" :key="result.DocId" :result="result" template-override="#vue-hawksearch-result-item-list"></result-item>
        </template>
        <template v-else>
            <placeholder-item v-for="index in 12" :key="index"></placeholder-item>
        </template>
    </div>
</script>

